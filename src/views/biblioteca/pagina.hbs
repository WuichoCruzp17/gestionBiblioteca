<div class="container-fluid">
    <form autocomplete="off">
        <div class="container-flat-form">
            <div class="title-flat-form title-flat-blue">
                Nueva Página.
            </div>
            <div class="row">
            <div class="col-xs-12 col-sm-8 col-sm-offset-2">
                <div class="group-material">
                    <input type="text" class="tooltips-general material-control" placeholder="Escribe el nombre de la página" required maxlength="20" data-toggle="tooltip" data-placement="top" title data-original-title="Escribe el nombre de la página.">
                    <span class="highlight"></span>
                    <span class="bar"></span>
                    <label>Nombre de Página</label>
                </div>
                <div class="group-material">
                    <input type="text" class="tooltips-general material-control" placeholder="Escribe el nombre de la página" required maxlength="100" data-toggle="tooltip" data-placement="top" title data-original-title="Escribe la url de la página.">
                    <span class="highlight"></span>
                    <span class="bar"></span>
                    <label>Url de la página</label>
                </div>
                <div class="group-material">
                    <span>Sección</span>
                    <select class="tooltips-general material-control" data-toggle="tooltip" data-placement="top" data-toggle="tooltip" data-placement="top" required data-original-title="Elige la categoría del libro">
                                    <option value="" disabled="" selected="">Selecciona una categoría</option>
                                    <option value="categoria">Categoría</option>
                                    <option value="categoria">Categoría</option>
                                    <option value="categoria">Categoría</option>
                                </select>
                </div>
                <div class="group-material">
                    <span>Incluir en el menú</span>
                    <input type="checkbox" class="material-control">
                </div>
                <div class="group-material">
                    <span>Estatus</span>
                    <select class="tooltips-general material-control" data-toggle="tooltip" data-placement="top" data-toggle="tooltip" data-placement="top" required data-original-title="Elige la categoría del libro">
                                    <option value="" disabled="" selected="">Selecciona una categoría</option>
                                    {{#each catalagoParametro}}
                                    <option value="{{catalogoParametroId}}">{{nombre}}</option>
                                    {{/each}}
                                </select>
                </div>
                <div class="group-material">
                    <span>Eliminado</span>
                    <select class="tooltips-general material-control" data-toggle="tooltip" data-placement="top" data-toggle="tooltip" data-placement="top" required data-original-title="Elige la categoría del libro">
                                    <option value="" disabled="" selected="">Selecciona una categoría</option>
                                    <option value="categoria">Eliminado</option>
                                    <option value="categoria">No Eliminado</option>
                                </select>
                </div>
                <div class="group-material">
                    <span>Fecha Creación</span>
                    <input type="text" name="fechaCreacion" class="material-control">
                </div>
                <div class="group-material">
                    <span>Usuario Creo</span>
                    <input type="text" name="usuarioCreao" class="material-control">
                </div>
                <div class="group-material">
                    <span>Fecha Modifico</span>
                    <input type="text" name="fechaModifico" class="material-control">
                </div>
                <div class="group-material">
                    <span>Usuario Modifico</span>
                    <input type="text" name="usuarioModifico" class="material-control">
                </div>
            </div>
            </div>
        </div>
    </form>
</div>